Comandos:

//Abaixo, execução do mapper versão1
!rm -r /content/output/shakespeare
!/usr/local/hadoop-3.3.4/bin/hadoop jar /usr/local/hadoop-3.3.4/share/hadoop/tools/lib/hadoop-streaming-3.3.4.jar -input /content/input/shakespeare/shakespeare.txt -output /content/output/shakespeare -file /content/mapper_v1.py  -file /content/reducer.py  -mapper 'python mapper_v1.py'  -reducer 'python reducer.py'

//Abaixo, execução do mapper versão2
!rm -r /content/output/shakespeare
!/usr/local/hadoop-3.3.4/bin/hadoop jar /usr/local/hadoop-3.3.4/share/hadoop/tools/lib/hadoop-streaming-3.3.4.jar -input /content/input/shakespeare/shakespeare.txt -output /content/output/shakespeare -file /content/mapper_v2.py  -file /content/reducer.py  -mapper 'python mapper_v2.py'  -reducer 'python reducer.py'

!cat /content/output/shakespeare/part-00000

!rm -r /content/output/p2p-gnutella-08 
